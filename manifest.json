{
    "manifest_version": 3,
    "name": "Remove Element Extension",
    "version": "1.0",
    "description": "指定されたセレクターを削除する拡張機能",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  